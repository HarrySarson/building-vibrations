\documentclass[12pt]{article}

\usepackage[margin=1in]{geometry} 
\usepackage{amsmath,amsthm,amssymb}
\usepackage{lscape}

\begin{document}
	
\begin{equation}
M\mathbf{\ddot{x}} + \Lambda\mathbf{\dot{x}} + K\mathbf{x} = \mathbf{f}
\end{equation}
	sadas
	
\begin{math}	
\begin{aligned}
\mathbf{x} &= 
\begin{bmatrix} 
	x_{f1} \\
	x_{f2} \\
	x_{f3} \\
	x_{a1} \\
	x_{a2} \\
	x_{a3} \\
\end{bmatrix} \\
\mathbf{f} &= 
\begin{bmatrix} 
	Fsin(\omega t) \\
	0 \\
	0 \\
	0 \\
	0 \\
	0 \\
\end{bmatrix} \\	
M &=
\begin{bmatrix} 
 m & 0 & 0 & 0 & 0 & 0 \\
 0 & m & 0 & 0 & 0 & 0 \\
 0 & 0 & m & 0 & 0 & 0 \\
 0 & 0 & 0 & m_{a1} & 0 & 0 \\
 0 & 0 & 0 & 0 & m_{a2} & 0 \\
 0 & 0 & 0 & 0 & 0 & m_{a3} \\
\end{bmatrix}  \\
\Lambda &= 
\begin{bmatrix} 
 2\lambda_{f} + \lambda_{a1} & -\lambda{f} & 0 & -\lambda_{a1} & 0 & 0 \\
 -\lambda_{f} & 2\lambda{f} + \lambda{a2} & -\lambda{f} & 0 & -\lambda{a2} & 0 \\
 0 & -\lambda{f} & \lambda{f} + \lambda{a3} & 0 & 0 & -\lambda{a3} \\
 -\lambda{f} & 0 & 0 & \lambda{a1} & 0 & 0 \\
 0 & -\lambda{f} & 0 & 0 & \lambda{a2} & 0 \\
 0 & 0 & -\lambda{f} & 0 & 0 & \lambda{a3} \\
 \end{bmatrix} \\
K &= 
\begin{bmatrix} 
 2k_{f} +  k_{a1} & -k_{f} & 0 & -k_{a1} & 0 & 0 \\
 -k_{f} & 2k_{f} + k_{a2} & -k{f} & 0 & -k_{a2} & 0 \\
 0 & -k_{f} & k_{f} + k_{a2} & 0 & 0 & -k_{a3} \\
 -k_{f} & 0 & 0 & k_{a1} & 0 & 0 \\
 0 & -k_{f} & 0 & 0 & k_{a2} & 0 \\
 0 & 0 & -k_{f} & 0 & 0 & k_{a3} \\
\end{bmatrix} \\
\end{aligned}
\end{math}


\end{document}          
